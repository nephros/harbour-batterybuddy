<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/daemon">
    <interface name="fi.kapsi.direc85.batterybuddy">
        <method name="enableCharging"></method>
        <method name="disableCharging"></method>
        <method name="isChargingEnabled">
            <arg name="enabled" type="b" direction="out" />
        </method>
        <!--
            <method name="canToggleCharging">
            <arg name="result" type="b" direction="out" />
            </method>
        -->
            <signal name="chargingEnabledChanged" />
            <property name="canToggleCharging" type="b" access="read"/>
        </interface>
    </node>
